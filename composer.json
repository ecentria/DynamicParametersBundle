{
    "name": "ecentria/dynamic-parameters-bundle",
    "description": "Runtime retrieval of parameters from environment variables for Symfony",
    "keywords": [
        "symfony parameters",
        "environment variable"
    ],
    "homepage": "https://github.com/ecentria/DynamicParametersBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Roman Shumkov",
            "email": "roman.shumkov@intexsys.lv"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/expression-language": "^2.7|^3.0",
        "symfony/http-kernel": "^2.7|^3.0",
        "symfony/config": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Incenteev\\DynamicParametersBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Incenteev\\DynamicParametersBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "conflict": {
        "incenteev/dynamic-parameters-bundle": "*",
        "symfony/symfony": ">=3.2",
        "symfony/dependency-injection": ">=3.2",
        "symfony/expression-language": ">=3.2",
        "symfony/http-kernel": ">=3.2",
        "symfony/config": ">=3.2",
        "symfony/yaml": ">=3.2"
    }
}
